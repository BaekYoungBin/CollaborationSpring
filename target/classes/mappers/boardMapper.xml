<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.grouping.board.mapper.BoardMapper">
	<select id="selectBoardList" parameterType="int" resultType="com.spring.grouping.board.domain.BoardVO">
		select * 
		from t_board_info
	</select>

	<select id="selectBoardListCnt" resultType="int">
		select count(*) 
		from t_board_info
	</select>
</mapper>
